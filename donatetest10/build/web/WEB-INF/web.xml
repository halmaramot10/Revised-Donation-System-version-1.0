<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.donate.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_Donation</servlet-name>
        <servlet-class>com.donate.Add_Donation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Pending</servlet-name>
        <servlet-class>com.donate.Update_Pending</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Released</servlet-name>
        <servlet-class>com.donate.Update_Released</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Clothes</servlet-name>
        <servlet-class>com.donate.Update_Clothes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Food</servlet-name>
        <servlet-class>com.donate.Update_Food</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Money</servlet-name>
        <servlet-class>com.donate.Update_Money</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Others</servlet-name>
        <servlet-class>com.donate.Update_Others</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_School</servlet-name>
        <servlet-class>com.donate.Update_School</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_Donation_Admin</servlet-name>
        <servlet-class>com.donate.Add_Donation_Admin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_Donation</servlet-name>
        <url-pattern>/Add_Donation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Pending</servlet-name>
        <url-pattern>/Update_Pending</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Released</servlet-name>
        <url-pattern>/Update_Released</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Clothes</servlet-name>
        <url-pattern>/Update_Clothes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Food</servlet-name>
        <url-pattern>/Update_Food</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Money</servlet-name>
        <url-pattern>/Update_Money</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Others</servlet-name>
        <url-pattern>/Update_Others</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_School</servlet-name>
        <url-pattern>/Update_School</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_Donation_Admin</servlet-name>
        <url-pattern>/Add_Donation_Admin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
